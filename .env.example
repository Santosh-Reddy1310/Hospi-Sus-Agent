# Example environment variables (do NOT commit real keys)
# Copy this file to `.env` and fill in your keys, or set env vars directly.

# Enable or disable LLM usage (0 = disable, 1 = enable)
ENABLE_LLM=0

# LLM provider: 'openai' or 'groq'
LLM_PROVIDER=groq

# Groq API key (do NOT commit):
GROQ_API_KEY=your_groq_api_key_here

# OpenAI API key (optional, if using openai provider)
OPENAI_API_KEY=
